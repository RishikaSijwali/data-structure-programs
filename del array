#include<stdio.h>
using namespace std;
int main()
{
	int pos,i,n,a[10];
	printf("enter n\n");
	scanf("%d",&n);
                 printf("n);
                 for(i=0;i<n;i++)
	{
		cin>>a[i];
	}
	printf("pos");
	scanf("%d",&pos);
	i=pos;
	while(i<n-1)
	{
		a[i]=a[i+1];
		i++;
	}
	for(i=0;i<n-1;i++)
	{
		printf("%d",a[i]);
	}
	return 0;
}
